{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "prettier/prettier": ["error"],
    "no-console": "off",
    "no-unused-vars": "warn",
    "consistent-return": "off",
    "func-names": "off",
    "arrow-body-style": "off",
    "object-shorthand": "warn",
    "no-underscore-dangle": "off"
  }
}
